# YouTube MP3 Downloader - Environment Variables

# Database Configuration
DATABASE_URL=postgresql://username:password@hostname:port/database

# Ad Network Configuration

# Google AdSense
VITE_GOOGLE_ADSENSE_CLIENT=ca-pub-xxxxxxxxxxxxxxxx
VITE_ADSENSE_HEADER_SLOT=1234567890
VITE_ADSENSE_CONTENT_SLOT=1234567891
VITE_ADSENSE_SIDEBAR_SLOT=1234567892
VITE_ADSENSE_FOOTER_SLOT=1234567893

# Media.net
VITE_MEDIANET_CID=your_medianet_cid_here

# Propeller Ads
VITE_PROPELLER_BANNER_ZONE=123456
VITE_PROPELLER_NATIVE_ZONE=123457
VITE_PROPELLER_INTERSTITIAL_ZONE=123458
VITE_PROPELLER_PUSH_ZONE=123459

# PopAds
VITE_POPADS_SITE_ID=123456
VITE_ENABLE_POPADS=false

# Amazon Associates
VITE_AMAZON_ASIN=B08N5WRWNW
VITE_AMAZON_LINK_ID=your_amazon_link_id

# Custom Ad Codes (HTML/JavaScript)
VITE_CUSTOM_HEADER_AD=<div>Your custom header ad code here</div>
VITE_CUSTOM_FOOTER_AD=<div>Your custom footer ad code here</div>
VITE_CUSTOM_SIDEBAR_AD=<div>Your custom sidebar ad code here</div>

# Ad Network Settings
VITE_ENABLE_ADS=true
VITE_AD_REFRESH_INTERVAL=30000